<%- include("header") -%>
    <title>Document</title>
  </head>
  <body>
    <section class="container-fluid">
      <%- include("navbar") -%>
      <div>
        <h1 class="top-title">
          Welcome to Data Storage and Analytics Platform of Govt. of Sikkim
        </h1>
        <h2></h2>
      </div>
      <form action="/districtPage" method="post">
        <div class="row">
          <div class="col-lg-12">
            <div class="card north" style="width: 18rem;">
              <div class="card-body">
                <h5 class="card-title">North Sikkim</h5>
                <button type="submit" name="districtBtn" value="North" class="btn btn-dark btn-lg">Go</button>
              </div>
            </div>
          </div>
        </div>
        <div class="row">
          <div class="col-lg-4">
            <div class="card west" style="width: 18rem;">
              <div class="card-body">
                <h5 class="card-title">West Sikkim</h5>
                <button type="submit" name="districtBtn" value="West" class="btn btn-dark btn-lg">Go</button>
              </div>
            </div>
          </div>
          <div class="col-lg-4">
            <img class="img" src="resources\sikkim-red.png" alt="" />
          </div>
          <div class="col-lg-4">
            <div class="card east" style="width: 18rem;">
              <div class="card-body">
                <h5 class="card-title">East Sikkim</h5>
                <!-- <h6 class="card-subtitle mb-2 text-muted">Card subtitle</h6> -->
                <button type="submit" name="districtBtn" value="East" class="btn btn-dark btn-lg">Go</button>
              </div>
            </div>
          </div>
        </div>
        <div class="row">
          <div class="col-lg-12">
            <div class="card south" style="width: 18rem;">
              <div class="card-body">
                <h5 class="card-title">South Sikkim</h5>
                <button type="submit" name="districtBtn" value="South" class="btn btn-dark btn-lg btn-index">Go</button>
              </div>
            </div>
          </div>
        </div>
      </form>
    </section>
    <script src="js\bootstrap.min.js"></script>
  </body>
</html>
